# 环境变量配置示例
# 复制此文件为 .env 并根据您的环境修改配置

# 服务器配置
APP_SERVER_HOST=127.0.0.1
APP_SERVER_PORT=8080

# MariaDB配置
APP_MARIADB_HOST=localhost
APP_MARIADB_PORT=3306
APP_MARIADB_DATABASE=device_data
APP_MARIADB_USERNAME=root
APP_MARIADB_PASSWORD=

# InfluxDB配置
APP_INFLUXDB_URL=http://localhost:8086
APP_INFLUXDB_DATABASE=device_signals
# APP_INFLUXDB_USERNAME=admin
# APP_INFLUXDB_PASSWORD=password

# Kafka配置
APP_KAFKA_BROKERS=localhost:9092
APP_KAFKA_TOPIC=device-signals
APP_KAFKA_CLIENT_ID=tauri-device-gateway